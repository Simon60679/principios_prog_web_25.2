    "@types/swagger-ui-express": "^4.1.8",
    "chai": "^6.2.2",
    "mocha": "^11.7.5",
    "sinon": "^21.0.1",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
